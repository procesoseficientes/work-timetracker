    "@types/mustache": "^4.1.0",
    "@types/pg": "^7.14.4",
    "@types/showdown": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.4"
