  "dependencies": {
    "bigojs": "^1.0.2",
    "color-hash": "^2.0.1",
    "cookie-parser": "~1.4.6",
    "copyfiles": "^2.4.1",
    "express": "~4.17.2",
    "express-handlebars": "^6.0.2",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.2",
    "json2csv": "^5.0.3",
    "json2xls": "^0.1.2",
    "md-to-pdf": "^5.0.2",
